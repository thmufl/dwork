type TimeInterval = 
 record {
   begin: Time;
   end: Time;
 };
type Time = int;
type CalendarEntry = 
 record {
   date: TimeInterval;
   description: text;
   id: nat32;
   title: text;
 };
service : {
  createEntry: (CalendarEntry) -> (nat32);
  deleteEntry: (principal, nat32) -> ();
  listEntries: (principal) -> (vec CalendarEntry) query;
  readEntry: (principal, nat32) -> (opt CalendarEntry) query;
  updateEntry: (CalendarEntry) -> (nat32);
}
